<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Event/etc/events.xsd">

    <event name="hawksearch_datafeed_generate_custom_feeds">
        <observer name="items_feed" instance="HawkSearch\Datafeed\Observer\Feeds\ItemFeed" />
        <observer name="labels_feed" instance="HawkSearch\Datafeed\Observer\Feeds\LabelFeed" />
        <observer name="attributes_feed" instance="HawkSearch\Datafeed\Observer\Feeds\AttributeFeed" />
        <observer name="content_feed" instance="HawkSearch\Datafeed\Observer\Feeds\ContentFeed" />
        <observer name="category_feed" instance="HawkSearch\Datafeed\Observer\Feeds\CategoryFeed" />
    </event>

</config>
